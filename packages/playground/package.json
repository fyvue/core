{
    "name": "playground",
    "version": "1.0.0",
    "scripts": {
        "dev:live": "vite --force --port 3000 ",
        "dev:bundle": "vite --force --port 3000 --config vite.config.bundle.ts",
        "build": "yarn run build:client && yarn run build:server:noExternal",
        "build:client": "vite build --ssrManifest --outDir dist/client --config vite.config.bundle.ts",
        "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server --config vite.config.bundle.ts",
        "build:server:noExternal": "vite build --config vite.config.noexternal.ts --ssr src/entry-server.ts --outDir dist/server",
        "typecheck": "vue-tsc --noEmit",
        "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore"
    },
    "dependencies": {
        "@fy-/components": "^0.1.7",
        "@fy-/core": "workspace:*",
        "@fy-/head": "^0.0.37",
        "@fy-/icons": "^0.0.2",
        "@karpeleslab/i18next-klb-backend": "^0.1.0",
        "@vuelidate/core": "^2.0.3",
        "@vuelidate/validators": "^2.0.3",
        "@vueuse/core": "^10.2.1",
        "ethers": "^6.6.2",
        "sitemap": "^7.1.1",
        "vue": "^3.3.4",
        "vue-code-highlight": "^0.7.8",
        "vue-router": "^4.2.4",
        "xml-formatter": "^3.4.1"
    },
    "devDependencies": {
        "@babel/types": "^7.22.5",
        "@headlessui/vue": "^1.7.14",
        "@heroicons/vue": "^2.0.18",
        "@karpeleslab/klbfw": "^0.1.13",
        "@rushstack/eslint-patch": "^1.3.2",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/line-clamp": "^0.4.4",
        "@tailwindcss/typography": "^0.5.9",
        "@types/node": "^20.4.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/server-renderer": "3.3.4",
        "@vue/tsconfig": "^0.4.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.44.0",
        "eslint-plugin-vue": "^9.15.1",
        "express": "^4.18.2",
        "i18next": "^23.2.8",
        "pinia": "^2.1.4",
        "postcss": "^8.4.25",
        "prettier": "2.8.8",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "sass": "^1.63.6",
        "tailwindcss": "^3.3.2",
        "typescript": "~5.1.6",
        "vite": "^4.4.2",
        "vue-eslint-parser": "^9.3.1",
        "vue-tsc": "^1.8.4"
    }
}